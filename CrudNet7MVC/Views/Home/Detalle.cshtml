@model CrudNet7MVC.Models.Contacto

@{
    ViewData["Title"] = "DETALLE USUARIO";
}

<h1 class="text-center">Contacto con .Net 7</h1>

<div class="row">
    <div class="offset-12">

        <div class="card border">
            <div class="card-header bg-dark">
                <h3 class="text-white"><strong>Lista de usuarios</strong> </h3>
            </div>
            <div class="card-body">
                <ul>
                    <li class="list-group-item"><strong>@Html.DisplayFor(m=> m.nombre)</strong></li>
                    <li class="list-group-item"><strong>@Html.DisplayFor(m=> m.telefono)</strong></li>
                    <li class="list-group-item"><strong>@Html.DisplayFor(m=> m.celular)</strong></li>
                    <li class="list-group-item"><strong>@Html.DisplayFor(m=> m.email)</strong></li>
                </ul>


                <a asp-controller="Home" asp-action="Index">Volver</a>


            </div>

        </div>

    </div>
</div>

